{
  "npmScope": "artifi",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "core": {
      "tags": []
    },
    "cli": {
      "tags": []
    },
    "cli-e2e": {
      "tags": [],
      "implicitDependencies": ["cli"]
    },
    "testreactlib": {
      "tags": []
    }
  }
}
